{
  "name": "gdpr-person-generator",
  "version": "0.0.26",
  "description": "Anonymization tool to comply with EU data protection law, GDRP",
  "main": "source/index.js",
  "types": "source/index.d.ts",
  "bugs": {
    "url": "https://github.com/pharrukh/gdpr-person-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pharrukh/gdpr-person-generator.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint './src/**/*.ts' --fix",
    "build": "tsc",
    "prepublish": "rm -rf ./source; npm run build"
  },
  "keywords": [
    "GDPR",
    "uzbekistan",
    "deterministic"
  ],
  "author": "Farrukh Normuradov",
  "license": "MIT",
  "files": [
    "source/**/*.js",
    "source/**/*.d.ts"
  ],
  "dependencies": {
    "seedrandom": "^2.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^26.0.24",
    "@types/seedrandom": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  }
}
